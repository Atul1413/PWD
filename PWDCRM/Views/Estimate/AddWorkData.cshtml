﻿
@{
    ViewData["Title"] = "AddWorkData";
    Layout = "~/Views/Shared/_Adminlte.cshtml";
    var data = ViewData["SubData"] as List<PWDCRM.Models.SubWorkDataDetail>;
    var listArea = ViewData["ListArea"] as List<PWDCRM.Models.AREA>;
}
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Main content -->
    <section class="content">
        <!-- Default box -->
        <div class="box">
            <div class="box-header with-border">
                <h3 class="box-title text-primary"><i class="fa fa-certificate"></i> @ViewData["Title"]</h3>
                <div class="box-tools pull-right">
                    <a class="btn btn-primary" href="@Url.Action("WorkData", "Estimate")">
                        Go Estimate List
                    </a>
                </div>
            </div>
            <div class="box-body">
                @using (@Html.BeginForm("AddWorkData", "Estimate", FormMethod.Post))
                {
                    @Html.AntiForgeryToken();
                    <div id="Grid">
                        <div class="row register-form">
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Name of Work</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" placeholder="Name of Work" maxlength="250" required="required" name="NameOfWork" id="txtNameOfWork" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Enter Total Sub Work</label>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" maxlength="3" oncopy="return false" onpaste="return false" onkeypress="if ( isNaN( String.fromCharCode(event.keyCode) )) return false;" placeholder="Enter Total Sub Work" required="required" name="SubWorkData" id="txtSubWorkData" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">SSR(Schedule of Rates)</label>
                                    <div class="col-md-6">
                                        <select class="form-control" required="required" id="ddlSSR" name="SSRRate">
                                            <option value="" class="hidden" selected disabled>Please select your SSR(Schedule of Rates)</option>
                                            <option value="SSR 17-18">SSR 17-18</option>
                                            <option value="SSR 18-19">SSR 18-19</option>
                                            <option selected value="SSR 19-20">SSR 19-20</option>
                                            <option value="SSR 20-21">SSR 20-21</option>
                                            <option value="SSR 21-22">SSR 21-22</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right"> Fund Head</label>
                                    <div class="col-md-6">
                                        <input list="ddlHeadOfAccounts" class="form-control" maxlength="250" required name="FundHead" id="ddlText5" />
                                        <datalist id="ddlHeadOfAccounts">
                                            <option value="Current Repairs Road and Bridges"></option>
                                            <option value="Special Repairs Road and Bridges"></option>
                                        </datalist>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Area</label>
                                    <div class="col-md-6">
                                        <select class="form-control" name="Area" id="ddlArea">
                                            <option value="" selected="selected" disabled="disabled">Select Area</option>
                                            @if (listArea != null)
                                            {
                                                if (listArea.Count != 0)
                                                {
                                                    foreach (var item in listArea)
                                                    {

                                                        <option value="@item.AName">@item.AName</option>

                                                    }
                                                }
                                            }
                                        </select>
                                        @*<input list="ddlArea" class="form-control" required   maxlength="250" name="Area" id="ddlText4" />
        <datalist id="ddlArea">
            <option value="Home Department"></option>
            <option value="Education Department"></option>
            <option value="Revenue Department"></option>
        </datalist>*@
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Cost of Diff</label>
                                    <div class="col-md-4">
                                        <input type="radio" name="CostDiff" value="Yes" id="CostDiff1" />&nbsp;<label for="CostDiff1">Yes</label> &nbsp;&nbsp;&nbsp;
                                        <input type="radio" name="CostDiff" value="No" id="CostDiff2" checked />&nbsp;<label for="CostDiff2">No</label>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Region</label>
                                    <div class="col-md-6">
                                        <input list="ddlRegion" class="form-control" required maxlength="250" value="Chief Engineer P.W. Region…"  name="Region" id="ddlText3" />
                                        <datalist id="ddlRegion">
                                            <option selected="selected" value="Chief Engineer P.W. Region…"></option>
                                        </datalist>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Circle</label>
                                    <div class="col-md-6">
                                        <input list="ddlCircle" class="form-control" required maxlength="250" value="Superintending Engineer P.W. Circle…."  name="Circle" id="ddlText2" />
                                        <datalist id="ddlCircle">
                                            <option selected="selected" value="Superintending Engineer P.W. Circle…."></option>
                                        </datalist>
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Divison</label>
                                    <div class="col-md-6">

                                        <input list="ddlDivison" class="form-control" required maxlength="250" value="Executive Engineer Public Works Division…." name="Divison" id="ddlText1" />
                                        <datalist id="ddlDivison">
                                            <option selected="selected" value="Executive Engineer Public Works Division…."></option>
                                        </datalist>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Sub-Divsion</label>
                                    <div class="col-md-6">
                                        <input list="ddlSub-Divsion" class="form-control" required maxlength="250" value="Sub-divisional engineer P.W. Sub division….." name="SubDivsion" id="ddlText" />
                                        <datalist id="ddlSub-Divsion">
                                            <option selected="selected" value="Sub-divisional engineer P.W. Sub division….."></option>
                                        </datalist>

                                    </div>
                                </div>


                                <div class="form-group row">
                                    <label class="col-md-4 col-form-label text-right">Remarks</label>
                                    <div class="col-md-8">
                                        <textarea class="form-control" id="txtRemarks" maxlength="500" name="Remarks"></textarea>
                                    </div>
                                </div>

                            </div>
                            <div class="col-md-6">
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right">Royalty</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control decimal" placeholder="Enter Royalty" maxlength="10" required="required" id="txtRoyalty" value=" 158.40" name="Royalty" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right">Type of SAND</label>
                                    <div class="col-md-5">
                                        <select class="form-control" required="required" id="ddlTypeSAND" name="TypeSAND">
                                            @*<option value="" class="hidden" selected disabled>Please select your Type SAND</option>*@
                                            <option value="R.A With Natural Sand" selected>R.A With Natural Sand  </option>
                                            <option value="R.A. With V.S.I Sand">R.A. With V.S.I Sand</option>
                                            <option value="R.A. With Natural Sand And V.S.I Sand Ratio">R.A. With Natural Sand And V.S.I Sand Ratio</option>
                                        </select>
                                    </div>
                                    <div class="col-md-3" id="addDivPer">
                                        <input type="text" class="form-control decimal" placeholder="SAND Percentage" maxlength="10" name="SANDPercentage" id="txtSANDPercentage" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right">V & QC Charges</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control decimal" maxlength="10" placeholder="V & QC Charges" value="0" name="VAndQCPercentage" id="txtVAndQCPercentage" />
                                    </div>
                                    <label class="col-md-3 col-form-label text-right">Internal Electrification</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control decimal" maxlength="10" placeholder="Internal Electrification" value="0" name="ElectricalINTPer" id="txtElectricalINTPer" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right">External Electrification</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control decimal" maxlength="10" placeholder="External Electrification" value="0" name="ElectricalExtPer" id="txtElectricalExtPer" />
                                    </div>
                                    <label class="col-md-3 col-form-label text-right">Water Supply Charges</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control decimal" maxlength="10" placeholder="Water Supply Charges" value="0" name="WaterSupply" id="txtWaterSupply" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right decimal"> Other Charges</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control decimal" maxlength="10" placeholder="Other Charges" value="0" name="AnyOthercharg1" id="txtAnyOthercharg1" />
                                    </div>
                                    <label class="col-md-3 col-form-label text-right"> Other Charges</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control decimal" maxlength="10" placeholder="Other Charges" value="0" name="AnyOthercharg2" id="txtAnyOthercharg2" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right">Labour insurance </label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control "  maxlength="50" placeholder="Labour insurance" value="1" name="LabourInsurance" id="txtLabourInsurance" />
                                    </div>
                                    <label class="col-md-3 col-form-label text-right">GST</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control" maxlength="50" placeholder="GST" value="12" required="required" name="GST" id="txtGST" />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-form-label text-right">Contingencie</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control" maxlength="100" placeholder="Contingencie" value="4" name="Contingencie" id="txtContingencie" />
                                    </div>
                                    <label class="col-md-3 col-form-label text-right">Centage Charges</label>
                                    <div class="col-md-3">
                                        <input type="text" class="form-control" maxlength="10" placeholder="Centage Charges" value="5" name="ServiceCharges" id="txtServiceCharges" />
                                    </div>
                                </div>

                                <fieldset>
                                    <legend>Billing </legend>
                                    <div class="form-group row">
                                        <label class="col-md-3 col-form-label text-right">Work Order No</label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" maxlength="50" placeholder="Work Order No" name="WorkOrderNo" id="txtWorkOrderNo" />
                                        </div>
                                        <label class="col-md-3 col-form-label text-right"> Tendered</label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control" placeholder="Tendered" name="Tendered" value="0" id="txtTendered" />
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-2 col-form-label text-right">Name of Agency</label>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" maxlength="250" placeholder="Name Of Agency" name="NameOfAgency" id="txtNameOfAgency" />
                                        </div>
                                        <label class="col-md-2 col-form-label text-right">Agreement No</label>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" maxlength="100" placeholder="Agreement No" name="AgreementNo" id="txtAgreementNo" />
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-md-2 col-form-label text-right">Approved by</label>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" maxlength="250" placeholder="Approved by" name="ApprovedBy" id="txtApprovedBy" />
                                        </div>
                                        <label class="col-md-2 col-form-label text-right">Prepared by </label>
                                        <div class="col-md-4">
                                            <input type="text" class="form-control" maxlength="100" placeholder="Prepared by" name="PreparedBy" id="txtPreparedBy" />
                                        </div>
                                    </div>

                                </fieldset>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group row">
                                    <div class="col-md-4 text-right">
                                        <a class="btn btn-danger" href="@Url.Action("WorkData", "Estimate")">Back </a>
                                    </div>
                                    <div class="col-md-4 text-right">
                                        <input type="submit" class="btn-primary btn btn-lg" id="btnSubmit" value="Create" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>
            <!-- /.box-body -->
            <div class="box-footer">
            </div>
            <!-- /.box-footer-->
        </div>
        <!-- /.box -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<script type="text/javascript">
    $(document).ready(function () {
        $("#addDivPer").hide();
    });
    $("#btnSubmit").click(function () {
        //var g = $("#ddlText").val();
        //alert(g);
    });
    $('.decimal').keypress(function (evt) {
        return (/^[0-9]*\.?[0-9]*$/).test($(this).val() + evt.key);
    });
</script>